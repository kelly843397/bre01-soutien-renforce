{% extends "base-admin.html.twig" %}
{% block body %}
<main class="container py-5">
            <h1 class="mb-5"></h1>
            <form action="index.php?route=admin-check-creer-avion" method="post" class="mt-3">
                <fieldset class="mb-4">
                    <label for="name" class="form-label">
                        Name
                    </label>
                    <input id="name" name="name" type="text" class="form-control" />
                </fieldset>
                <fieldset class="mb-4">
                    <label for="start_year" class="form-label">
                        Année de mise en service
                    </label>
                    <input id="start_year" name="start_year" type="number" class="form-control" />
                </fieldset>
                <fieldset class="mb-4">
                    <label for="end_year" class="form-label">
                        Année de fin de service
                    </label>
                    <input id="end_year" name="end_year" type="number" class="form-control" />
                </fieldset>
                <fieldset class="mb-4">
                    <label for="picture_url" class="form-label">
                        Url de l'image
                    </label>
                    <input id="picture_url" name="picture_url" type="text" class="form-control" />
                </fieldset>
                <fieldset>
                    <button type="submit" class="btn btn-primary">Enregistrer</button>
                </fieldset>
                </form>
                
</main>
{% endblock %}